.my-value-cell{
  padding-left:38rpx;
  background: #fff;
}
.my-value-cell .left, .my-value-cell .right{
  display: inline-block;
}
.my-value-cell .left{
  width: 84rpx;
  height: 84rpx;
}
.my-value-cell .right{
  width:540rpx;
  margin-left:38rpx;
  font-size:24rpx;
  color:#B2B2B2;
  vertical-align:top;
}
.my-value-cell .right .rightTop{
  color: #252525;
  font-size:28rpx;
  margin-bottom:10rpx;
  padding-top:5rpx;
}
.my-value-cell image{
  width: 100%;
  height: 100%;
}
.my-value-cell .content{
  border-top: 1rpx solid #dcdcdc;
  height: 160rpx;
  box-sizing: border-box;
  padding: 38rpx;
  padding-left: 0;
}
.my-value-cell .right .rightTop .value{
  float: right;
  font-size: 36rpx;
}
.my-value-cell-temp .my-value-cell:first-child .content{
  border:0;
}