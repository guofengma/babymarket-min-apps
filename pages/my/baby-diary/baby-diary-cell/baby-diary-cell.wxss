.baby-diary-cell {
  display: flex;
  flex-direction: column;
  width: 47%;
  background-color: white;
  margin: 10rpx;
  padding-bottom: 20rpx;
}

.cell-image {
  width: 100%;
  height: 300rpx;
  background-color: #f3f4f5;
  flex: none;
}

.date-age {
  display: flex;
  align-items: center;
  padding: 10rpx;
}

.date {
  font-size: 26rpx;
  color: #494848;
}

.age {
  font-size: 24rpx;
  color: #acadaa;
  margin-left: 10rpx;
}

.content {
  height: 72rpx;
  padding: 0rpx 10rpx;
  font-size: 26rpx;
  color: #494848;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
