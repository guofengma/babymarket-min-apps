.question-base-cell{
    background: white;
    padding: 20rpx 30rpx 35rpx 22rpx;
}

.question-base-cell .top-view{
    height: 40rpx;
    font-size: 28rpx;
    color: #acadaa;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.question-base-cell .top-view .left-view{
    display: flex;
    align-items: center;
}

.question-base-cell .content{
    font-size: 32rpx;
    color: #090909;
    margin-top: 20rpx;
}

.top-view .name{
    margin-left: 15rpx;
}