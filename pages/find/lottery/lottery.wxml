<!--lottery.wxml-->
<import src='/pages/find/lottery/lottery-cell/lottery-cell.wxml' />

<view class="lottery-view">
    <image src="/res/img/find/find-lottery-bg-view-icon.png" class="bg-view"></image>
    <view class="my-coins">我的金币：1000</view>
    <view class="tips">每次抽奖将消耗10金币</view>

    <view class="circle-view">
        <block wx:for="{{lotteryDatas}}" wx:key='unique'>
            <template is="lottery-cell" data="{{index:index}}"></template>
        </block>
    </view>

</view>
