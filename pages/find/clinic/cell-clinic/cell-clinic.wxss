@import '/pages/find/head/head.wxss';

.clinic-item {
    background: white;
    display: flex;
    flex-direction: column;
    padding: 20rpx 20rpx 20rpx 20rpx;
    border-bottom: 20rpx solid #fff8f4;
}

.clinic-value {
    margin-top: 30rpx;
    font-size: 32rpx;
    color: #090909;
}

.clinic-time {
    margin-top: 40rpx;
    font-size: 24rpx;
    color: #acadaa;
    align-self: flex-end;
}
