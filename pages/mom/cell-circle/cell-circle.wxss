.cell-circle {
  display: flex;
  background: white;
  align-items: center;
  padding: 20rpx;
}

.cell-circle .head-img {
  width: 104rpx;
  height: 104rpx;
  border-radius: 40rpx;
  flex: none;
}

.cell-circle .info {
  display: flex;
  flex-grow: 1;
  flex-direction: column;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.cell-circle .info .name {
  font-size: 30rpx;
  color: #090909;
}

.cell-circle .info .content {
  font-size: 26rpx;
  color: #494848;
  margin-top: 5rpx;
}

.cell-circle .status {
  width: 122rpx;
  height: 44rpx;
  flex: none;
}
