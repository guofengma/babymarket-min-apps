<import src='/pages/my/my-create-post/cell/my-create-post-cell.wxml' />

<template name="mom-item">
    <view class="mom-item" catchtap="goDetail" data-index="{{index}}">
        <template is="my-create-post-cell" data="{{item, isShowReadNum:false}}"></template>
        <view class="bottom">
            <view class="bottom-left">
                <image class="icon-read" src="/res/img/mother-circle/mother-circle-eye-icon.png" mode="aspectFit"></image>
                <view class="num-read">{{item.TotalViews}}</view>
            </view>
            <view class="bottom-right">
                <image class="icon-message" src="/res/img/mother-circle/mother-circle-comment-icon.png" mode="aspectFit"></image>
                <view class="num-message">全部评论{{item.Commemt_Number}}</view>
            </view>
        </view>
      </view>
</template>